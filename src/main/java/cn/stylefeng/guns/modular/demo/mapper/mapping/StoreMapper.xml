<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.demo.mapper.StoreMapper">

    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.demo.entity.Store">
            <id property="storeId" column="store_id" jdbcType="BIGINT"/>
            <result property="storeName" column="store_name" jdbcType="VARCHAR"/>
            <result property="storeAddress" column="store_address" jdbcType="VARCHAR"/>
            <result property="storeCode" column="store_code" jdbcType="VARCHAR"/>
            <result property="manuId" column="manu_id" jdbcType="BIGINT"/>
            <result property="delFlag" column="del_flag" jdbcType="CHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="createUser" column="create_user" jdbcType="VARCHAR"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="updateUser" column="update_user" jdbcType="VARCHAR"/>
            <result property="storeStat" column="store_stat" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        store_id,store_name,store_address,
        store_code,manu_id,del_flag,
        create_time,create_user,update_time,
        update_user,store_stat
    </sql>
</mapper>
